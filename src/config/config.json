{
  "db": {
    "location": "/data/db_meta/leveldb",
    "options": {
      "createIfMissing": true,
      "errorIfExists": false,
      "compression": true,
      "cacheSize": 8388608
    }
  },
  "fuse": {
    "name": "main",
    "mnt": "./mnt",
    "options": [ "direct_io", "max_read=131072", "max_write=131072"]
  },
  "cmd_server": {
    "port": 8088
  }
}
