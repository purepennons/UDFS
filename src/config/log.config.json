{
  "appenders": [
      {
          "type": "file",
          "filename": "fuse.log",
          "maxLogSize": 10485760,
          "backups": 2,
          "absolute": true,
          "category": [ "handlers", "io", "cpu", "memory", "get_dest" ],
          "layout": {
            "type": "pattern",
            "pattern": "%c, %d{ABSOLUTE}, %m"
          }
      },
      {
          "type": "console",
          "category": [ "handlers", "io", "cpu", "memory", "get_dest" ]
      }
  ],
  "replaceConsole": false
}
